{
  "name": "about-me-react",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@douyinfe/semi-icons": "2.87.1",
    "@douyinfe/semi-illustrations": "2.87.0-alpha.8",
    "@douyinfe/semi-ui-19": "2.87.0-alpha.8",
    "@emotion/is-prop-valid": "^1.4.0",
    "@giscus/react": "^3.1.0",
    "@hi-ui/watermark": "^4.0.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-responsive": "^10.0.1",
    "react-router": "^7.9.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.8"
  },
  "scripts": {
    "start": "rsbuild dev",
    "dev": "rsbuild dev",
    "prebuild": "node src/scripts/fetchWakatimeData.js",
    "getgitinfo": "node src/scripts/generateBuildInfo.js",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "analysis": "RSDOCTOR=true npm run build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "include": [
      "/",
      "/urls",
      "/tools"
    ],
    "source": "build",
    "puppeteerConfig": {
      "headless": true,
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-gpu",
        "--disable-software-rasterizer"
      ]
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@rsbuild/core": "^1.5.15",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rsdoctor/rspack-plugin": "^1.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22.18.8",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "prettier": "3.6.2",
    "raw-loader": "^4.0.2",
    "react-snap": "^1.23.0",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3"
  }
}
