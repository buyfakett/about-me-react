{
  "name": "about-me-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@douyinfe/semi-icons": "^2.72.0",
    "@douyinfe/semi-ui": "^2.72.0",
    "@hi-ui/watermark": "^4.0.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "framer-motion": "^11.10.0",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "python3 src/scripts/fetchWakatimeData.py",
    "getgitinfo": "node src/scripts/generateBuildInfo.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "include": [
      "/",
      "/urls",
      "/tools"
    ],
    "source": "build",
    "puppeteerConfig": {
      "headless": true,
      "args": [
        "--no-sandbox",
        "--disable-setuid-sandbox",
        "--disable-gpu",
        "--disable-software-rasterizer"
      ]
    }
  },
  "devDependencies": {
    "react-scripts": "^5.0.1",
    "react-snap": "^1.23.0",
    "typescript": "^4.9.5"
  }
}
